                 RowBox[{"b1", "[", 
                  RowBox[{"[", "3", "]"}], "]"}], 
                 RowBox[{"b2", "[", 
                  RowBox[{"[", "3", "]"}], "]"}], "z"}
               }], ")"}], "]"}]}], ";", "\[IndentingNewLine]", 
           RowBox[{"Return", "[", 
            RowBox[{"{", "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{
               RowBox[{"m", "[", 
                RowBox[{"[", "1", "]"}], "]"}], "[", 
