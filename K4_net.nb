       " ", "*)"}], "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"origin", "=", 
        RowBox[{"facet2", "[", 
         RowBox[{"[", "1", "]"}], "]"}]}], ";", " ", 
       RowBox[{"(*", " ", 
        RowBox[{
        "choose", " ", "arbitrary", " ", "origin", " ", "in", " ", "plan"}], 
        " ", "*)"}], "\[IndentingNewLine]", 
       RowBox[{"facet2Linear", "=", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"facet2", "[", 
            RowBox[{"[", "i", "]"}], "]"}], "-", "origin"}], ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", "1", ",", 
            RowBox[{"Length", "[", "facet2", "]"}]}], "}"}]}], "]"}]}], ";", 
       " ", 
       RowBox[{"(*", " ", 
        RowBox[{"translate", " ", "to", " ", "origin"}], " ", "*)"}], 
