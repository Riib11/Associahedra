       "\[IndentingNewLine]", "\[IndentingNewLine]", 
       RowBox[{"(*", 
        RowBox[{
         RowBox[{"Print", "[", 
          RowBox[{"{", 
           RowBox[{"b1", ",", "b2"}], "}"}], "]"}], ";"}], "*)"}], 
       "\[IndentingNewLine]", "\[IndentingNewLine]", 
       RowBox[{"(*", "\[IndentingNewLine]", " ", 
        RowBox[{
         RowBox[{
         "*", " ", "Convert", " ", "vector3", " ", "to", " ", "vector2"}], 
         ",", " ", 
         RowBox[{"in", " ", "terms", " ", "of", " ", "basis2"}]}], 
        "\[IndentingNewLine]", " ", "*)"}], "\[IndentingNewLine]", 
       RowBox[{"(*", " ", 
        RowBox[{
         RowBox[{"unfold", " ", "vertex", " ", "into", " ", "2", "D"}], ",", 
         " ", 
         RowBox[{"in", " ", "terms", " ", "of", " ", "basis"}]}], " ", "*)"}],
        "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"unfoldVertex", "[", 
         RowBox[{"{", 
          RowBox[{"x_", ",", "y_", ",", "z_"}], "}"}], "]"}], ":=", 
        RowBox[{"Block", "[", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"{", "m", "}"}], ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"m", "=", 
            RowBox[{"RowReduce", "[", 
             RowBox[{"(", GridBox[{
                {
                 RowBox[{"b1", "[", 
                  RowBox[{"[", "1", "]"}], "]"}], 
                 RowBox[{"b2", "[", 
                  RowBox[{"[", "1", "]"}], "]"}], "x"},
